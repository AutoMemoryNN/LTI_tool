{
    "name": "lti-tool",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "build": "remix vite:build",
        "dev": "remix vite:dev",
        "start": "remix-serve ./build/server/index.js",
        "typecheck": "tsc",
        "check": "biome check . && tsc --noEmit",
        "format": "biome format --write ."
    },
    "dependencies": {
        "@remix-run/node": "2.16.8",
        "@remix-run/react": "2.16.8",
        "@remix-run/serve": "2.16.8",
        "isbot": "4.1.0",
        "jose": "6.0.12",
        "postcss": "8.5.6",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.1.1",
        "@remix-run/dev": "2.16.8",
        "@types/react": "18.2.20",
        "@types/react-dom": "18.2.7",
        "autoprefixer": "10.4.19",
        "tailwindcss": "3.4.4",
        "typescript": "5.1.6",
        "vite": "5.4.19",
        "vite-tsconfig-paths": "4.2.1"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
